{
  "name": "bitsensor/php",
  "description": "BitSensor Web Application Security",
  "keywords": [
    "security",
    "insight"
  ],
  "homepage": "https://bitsensor.io/",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "BitSensor",
      "email": "info@bitsensor.nl"
    },
    {
      "name": "Sven Dubbeld",
      "email": "sven@bitsensor.io",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "support@bitsensor.io",
    "issues": "https://git.bitsensor.io/plugins/php/issues",
    "source": "https://git.bitsensor.io/plugins/php"
  },
  "require": {
    "php": ">=5.6.0",
    "google/protobuf": "^v3.5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.*"
  },
  "autoload": {
    "psr-4": {
      "BitSensor\\": "src/",
      "GPBMetadata\\": "src/Core/GPBMetadata/",
      "Proto\\": "src/Core/Proto/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BitSensor\\Test\\": "test/"
    }
  },
  "config": {
    "platform": {
      "php": "5.6.0"
    }
  }
}
